<!doctype html>
<html lang="en">

<head>
  <title>Battleship Online - Dashboard</title>
  <% include ../partials/head %>

    <link rel="stylesheet" href="../css/index.css">
    <script src="../js/index.js"></script>
</head>

<body>
  <header role="banner">
    <h1>Battleship online</h1>
  </header>
  <main role="main">
    <section class="player-info">

    </section>

    <section class="new-game-section" class="not-visible">
      <button type="button" class="new-battle">
        <i class="fas fa-bomb"></i> New Battle!</button>
      <span class="link not-visible">
        <a class="join-game-link"></a>
        <button type="button" class="copy" id="copy">
          <i class="far fa-copy"></i>
        </button>
      </span>
    </section>
    <div class="error-message"></div>
  </main>
</body>

</html>